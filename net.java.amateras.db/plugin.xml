<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension-point id="dialects" name="dialects" schema="schema/dialects.exsd"/>
   <extension-point id="generators" name="generators" schema="schema/generators.exsd"/>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="net.java.amateras.db.visual.editor.VisualDBEditor"
            contributorClass="net.java.amateras.db.visual.editor.VisualDBEditorContributor"
            default="true"
            extensions="erd"
            icon="icons/table.gif"
            id="net.java.amateras.db.editor1"
            name="%plugin.erd.editor"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="net.java.amateras.db"
            name="%plugin.name"
            />
      <wizard
            category="net.java.amateras.db"
            class="net.java.amateras.db.wizard.NewDiagramWizard"
            icon="icons/table.gif"
            id="net.java.amateras.db.newDiagramWizard"
            name="%plugin.erd.wizard"/>
   </extension>
   <extension
         point="net.java.amateras.db.dialects">
      <dialect
            class="net.java.amateras.db.dialect.HsqldbDialect"
            name="hsqldb"/>
      <dialect
            class="net.java.amateras.db.dialect.H2Dialect"
            name="H2"/>
	  <dialect
            class="net.java.amateras.db.dialect.DerbyDialect"
            name="Derby"/>
	  <dialect
	        class="net.java.amateras.db.dialect.MySQLDialect"
	        name="MySQL"/>
	  <dialect
	        class="net.java.amateras.db.dialect.PostgreSQLDialect"
	        name="PostgreSQL"/>
	  <dialect
	        class="net.java.amateras.db.dialect.OracleDialect"
	        name="Oracle"/>
	 <dialect
	        class="net.java.amateras.db.dialect.MSSQLDialect"
	        name="MSSQL"/>
	 <dialect
	        class="net.java.amateras.db.dialect.SybaseDialect"
	        name="Sybase"/>
	<dialect
	        class="net.java.amateras.db.dialect.DB2Dialect"
	        name="DB2">
  </dialect>
   </extension>
   <extension
         point="net.java.amateras.db.generators">
      <generator
            class="net.java.amateras.db.visual.generate.DDLGenerater">
      </generator>
      <!--
      <generator
            class="net.java.amateras.db.visual.generate.DFGenerater">
      </generator>
      -->
      <generator
            class="net.java.amateras.db.visual.generate.HTMLGenerator">
      </generator>
      <generator
            class="net.java.amateras.db.visual.generate.ExcelGenerator">
      </generator>
      <generator
            class="net.java.amateras.db.visual.generate.ImageGenerator">
      </generator>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="net.java.amateras.db.preference.ERDPreferencePage"
            name="AmaterasERD"
            id="net.java.amateras.db.preference.ERDPreferencePage"/>
      <page
            class="net.java.amateras.db.preference.ValidationPreferencePage"
            name="%plugin.erd.preference.validation"
            id="net.java.amateras.db.preference.ValidationPreferencePage"
            category="net.java.amateras.db.preference.ERDPreferencePage"/>
      <page
            class="net.java.amateras.db.preference.DictionaryPreferencePage"
            name="%plugin.erd.preference.dictionary"
            id="net.java.amateras.db.preference.DictionaryPreferencePage"
            category="net.java.amateras.db.preference.ERDPreferencePage"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="net.java.amateras.db.preference.DBPreferenceInitializer">
      </initializer>
   </extension>
</plugin>
